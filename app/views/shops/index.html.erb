<p id="notice"><%= notice %></p>

<h1>Shops</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Suburb</th>
      <th>Postcode</th>
      <th>Contact</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shops.each do |shop| %>
      <tr>
        <td><%= shop.name %></td>
        <td><%= shop.address %></td>
        <td><%= shop.suburb %></td>
        <td><%= shop.postcode %></td>
        <td><%= shop.contact %></td>
        <td><%= shop.phone %></td>
        <td><%= shop.email %></td>
        <td><%= shop.note %></td>
        <td><%= link_to 'Show', shop %></td>
        <td><%= link_to 'Edit', edit_shop_path(shop) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shop', new_shop_path %>
